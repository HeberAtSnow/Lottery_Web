{

  "AllowedHosts": "*",
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Error"
    }
  },
  "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
  "WriteTo": [
    {
      "Name": "File",
      "Args": {
        "path": "C:\\Desktop\\Logs\\Access.log",
        "outputTemplate": "{Timestamp:G} {Message}{NewLine:1}{Exception:1}"
      }

    },
    {
      "Name": "File",
      "Args": {
        "path": "C:\\Desktop\\Logs\\AppPerformance.log",
        "outputTemplate": "{Timestamp:G} {Message}{NewLine:1}{Exception:1}"
      }
    }
  ]
}
