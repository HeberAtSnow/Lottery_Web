{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Expressions", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            //"Filter": [
            //  {
            //    "Name": "ByIncludingOnly",
            //    "Args": {
            //      "expression": "File = '[ACC]'"
            //    }
            //  }
            //],
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "Logs/Access.log"
                }
              }
            ]
          }
        }
      },
      {
        "Name": "seq"
      }
    ]
  }
}